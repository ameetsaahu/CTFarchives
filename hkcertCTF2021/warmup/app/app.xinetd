service app
{
    disable = no
    socket_type = stream
    protocol = tcp
    wait = no
    type = UNLISTED
    bind = 0.0.0.0
    port = 1337
    log_type = FILE /proc/1/fd/1
    log_on_success = HOST PID DURATION EXIT
    log_on_failure = HOST ATTEMPT
    user = warmup

    server = /home/warmup/chall

    per_source = 20
}
